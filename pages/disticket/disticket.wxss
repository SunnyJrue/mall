.tap{
    display:flex;
    display:-webkit-flex;
}
.tap view{
    width:33.33%;
    text-align:center;
    height:80rpx;
    line-height:80rpx;
    background:#fff;
    font-size:28rpx;
}
.active{
    border-bottom:4rpx solid #e30c26;
}
.ticket image{
    width:200rpx;
    height:200rpx;
}
.noticket{
    font-size:28rpx;
    width:300rpx;
    height:500rpx;
    text-align:center;
    line-height:500rpx;
    margin:0 auto;
    background:url('../../images/icon-ticket.png') no-repeat center 50rpx;
    background-size:150rpx 150rpx;
}